<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>huadian</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.huadian.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.huadian.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/Register</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>showServlet</servlet-name>
		<servlet-class>com.huadian.ShowServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showServlet</servlet-name>
		<url-pattern>/frame/Show</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addTocar</servlet-name>
		<servlet-class>com.huadian.Addtocar</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addTocar</servlet-name>
		<url-pattern>/frame/Add</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>showcar</servlet-name>
		<servlet-class>com.huadian.Showcar</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>showcar</servlet-name>
		<url-pattern>/frame/Showcar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>admin</servlet-name>
		<servlet-class>com.huadian.AdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>admin</servlet-name>
		<url-pattern>/frame/Admin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>change</servlet-name>
		<servlet-class>com.huadian.ChangeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>change</servlet-name>
		<url-pattern>/frame/Change</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Subcar</servlet-name>
		<servlet-class>com.huadian.SubServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Subcar</servlet-name>
		<url-pattern>/frame/Sub</url-pattern>
	</servlet-mapping>
</web-app>